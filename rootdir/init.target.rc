service SMD-daemon /system/bin/smdexe
    class main
    user root
    seclabel u:r:SMD-daemon:s0
    group system radio inet net_raw

# this service use /dev/mdm
service qc_kickstart /system/bin/qcks s
    class core
    user root
    seclabel u:r:kickstart:s0
    group radio cache inet misc audio sdcard_rw log

## For EncryptionMode - remove disabled, Modify class main
service qmiproxy /system/bin/qmiproxy
    class main
    user radio
    seclabel u:r:qmiproxy:s0
    group radio gps

# this service use /dev/hsic**
service qmuxd /system/bin/qmuxd
    class main
    user system
    seclabel u:r:qmuxd:s0
    group radio log audio bluetooth gps log



